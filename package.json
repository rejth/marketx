{
  "name": "marketx",
  "version": "0.1.0",
  "description": "Market web application",
  "main": "index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rejth/marketx.git"
  },
  "author": "Ilya Kirsanov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rejth/marketx/issues"
  },
  "scripts": {
    "dev": "webpack serve --env port=3000",
    "build": "webpack  --env mode=production",
    "predeploy": "pnpm build",
    "deploy": "gh-pages -d build",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "lint:js:check": "npx eslint \"src/**/*.{ts,tsx,js}\"",
    "lint:js:fix": "npx eslint \"src/**/*.{ts,tsx,js}\" --fix",
    "lint:css:check": "npx stylelint \"src/**/*.{css,scss,sass}\" --custom-syntax postcss-scss",
    "lint:css:fix": "npx stylelint \"src/**/*.{css,scss,sass}\" --fix --custom-syntax postcss-scss",
    "prettier:fix": "prettier src/**/*.{ts,tsx} --write",
    "check-updates": "npx npm-check-updates",
    "update-packages": "npx npm-check-updates -u",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{css,scss,sass}": [
      "pnpm lint:css:fix"
    ],
    "src/**/*.{ts,tsx,js}": [
      "pnpm prettier:fix",
      "pnpm lint:js:fix"
    ]
  },
  "commit-msg": {
    "regex": "(feat|fix|docs|style|refactor|test|chore)(:\\s)([a-z]{1,})",
    "error-message": "Need message like: feat: add test for new page. Read Semantic Commit Messages"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/circular-dependency-plugin": "^5.0.5",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "circular-dependency-plugin": "^5.2.2",
    "commit-msg-regex": "^1.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^7.2.14",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-html-reporters": "^3.0.11",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "npm-check-updates": "^16.6.2",
    "postcss": "^8.4.20",
    "postcss-csso": "^6.0.1",
    "postcss-loader": "^7.0.2",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.1",
    "react-refresh": "^0.14.0",
    "regenerator-runtime": "^0.13.11",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-group-selectors": "^1.0.9",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
